<div class='container'>
  <% @departments.each do |department, entries| %>
  <div id=<%= department %> class='section scrollspy'>
    <h2><%= department %></h2>
    <table class='highlight'>
      <thead>
        <tr style='text-align:left;'>
          <th></th>
          <th>PART</th>
          <th>SKU</th>
          <th>CATEGORY</th>
          <th>PRICE</th>
          <th>DESCRIPTION</th>
          <th>OVERALL DESCRIPTION</th>
        </tr>
      </thead>
      <% entries.each do |entry| %>
      <tbody>
        <tr>
          <td><img src='/img/<%= entry['ml_img'] %>' style='max-height:36px;max-width:36px;filter:grayscale(1);'></td>
          <td style='font-size:10px;'><%= entry['title'] %></td>
          <td style='font-size:10px;'><%= entry['sku'] %></td>
          <td style='font-size:10px;'><%= entry['category'] %></td>
          <td style='font-size:10px;'><%= entry['price'] %></td>
          <td style='font-size:10px'><%= entry['description'] %></td>
          <td style='font-size:10px;'><%= entry['overview_description'] %></td>
        </tr>
      </tbody>
      <% end %>
    </table>
  </div>
  <br>
  <% end %>
</div>
