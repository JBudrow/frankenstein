$(document).one("touchmove mousemove scroll",function(){var i=encodeURIComponent(document.domain),r=$("meta[name=riskified-settings]").data("riskified-shopper"),u=("https:"==document.location.protocol?"https://":"http://")+"beacon.riskified.com?shop="+i+"&sid="+r,n=document.createElement("script"),t;n.id="riskloader";n.type="text/javascript";n.async=!1;n.defer=!0;n.src=u;t=document.getElementById("riskBeacon");document.getElementById("riskloader")||t.parentNode.insertBefore(n,t)})